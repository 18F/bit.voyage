{
  "name": "bit-voyage",
  "version": "0.1.0",
  "description": "Send your bits on a voyage.",
  "author": "Eric Mill",
  "license": "CC0",
  "repository": {
    "type": "git",
    "url": "https://github.com/konklone/bit-voyage.git"
  },
  "bugs": {
    "url": "https://github.com/konklone/bit-voyage/issues"
  },
  "homepage": "https://github.com/konklone/bit-voyage",

  "dependencies": {
    "filereader-stream": "git://github.com/konklone/filereader-stream.git#backpressure",
    "s3-upload-stream": "*",
    "aws-sdk": "*"
  },

  "main": "index.js",
  "scripts": {
    "compile": "browserify -r ./assets/js/echo.js:echo.js -r filereader-stream -r ./assets/js/params.js:params.js -r aws-sdk -r ./assets/js/drop.js:drop.js -r ./assets/js/log.js:log.js -r s3-upload-stream > assets/js/bundle.js",
    "watch": "watchify -r ./assets/js/echo.js:echo.js -r filereader-stream -r ./assets/js/params.js:params.js -r aws-sdk -r ./assets/js/drop.js:drop.js -r ./assets/js/log.js:log.js -r s3-upload-stream -o assets/js/bundle.js"
  }
}
