  "license": "CC0",
  "repository": {
    "type": "git",
    "url": "https://github.com/konklone/upload-anything.git"
  },
  "bugs": {
    "url": "https://github.com/konklone/upload-anything/issues"
  },
  "homepage": "https://github.com/konklone/upload-anything",

  "dependencies": {
    "filereader-stream": "*",
    "s3-upload-stream": "git://github.com/konklone/s3-upload-stream.git#browser-patch"
  },

  "main": "index.js",
  "scripts": {
    "compile": "browserify -r filereader-stream -r ./assets/js/params.js:params.js -r ./assets/js/aws-sdk-2.0.19.min.js:aws-sdk-2.0.19.min.js -r ./assets/js/drop.js:drop.js -r ./assets/js/log.js:log.js -r s3-upload-stream > assets/js/bundle.js",
    "watch": "watchify -r filereader-stream -r ./assets/js/params.js:params.js -r ./assets/js/aws-sdk-2.0.19.min.js:aws-sdk-2.0.19.min.js -r ./assets/js/drop.js:drop.js -r ./assets/js/log.js:log.js -r s3-upload-stream -o assets/js/bundle.js"
  }
}
